{
	"version": 3,
	"routes": [
		{
			"src": "/live-now",
			"dest": "/live-now/"
		},
		{
			"src": "/live-now/",
			"status": 308,
			"headers": {
				"Location": "/live-now"
			}
		},
		{
			"src": "/_app/immutable/.+",
			"headers": {
				"cache-control": "public, immutable, max-age=31536000"
			}
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "/.*",
			"dest": "/fn"
		}
	],
	"overrides": {
		"index.html": {
			"path": ""
		},
		"live-now.html": {
			"path": "live-now"
		}
	}
}